<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.example.demo.mapper.MyPageMapper">
	<select id="aptInfo" parameterType="map" resultType="com.example.demo.model.AptInfoDTO">
		select * from apt_info where apt like concat('%',#{apt},'%') and dong_num like concat('%',#{dong_num},'%')
																		<!-- dong like concat('%',#{dong},'%') -->
	</select>
	
</mapper>